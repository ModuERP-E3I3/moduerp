<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="QualityControlMapper">

	<resultMap id="qualityControlResult"
		type="com.e3i3.moduerp.quality.model.dto.QualityControlDTO">
		<result property="inspecCode" column="INSPEC_CODE" />
		<result property="orderNumber" column="ORDER_NUMBER" />
		<result property="uuid" column="UUID" />
		<result property="departmentId" column="DEPARTMENT_ID" />
		<result property="startDate" column="START_DATE" />
		<result property="endDate" column="END_DATE" />
		<result property="inspecType" column="INSPEC_TYPE" />
		<result property="progressStatus" column="PROGRESS_STATUS" />
		<result property="inspecResult" column="INSPEC_RESULT" />
		<result property="inspecQty" column="INSPEC_QTY" />
	</resultMap>

	<select id="selectAllQualityControls" resultMap="qualityControlResult">
		select
		INSPEC_CODE,
		ORDER_NUMBER,
		UUID,
		DEPARTMENT_ID,
		START_DATE,
		END_DATE,
		INSPEC_TYPE,
		PROGRESS_STATUS,
		INSPEC_RESULT,
		INSPEC_QTY
		from QUALITY_CONTROL
	</select>

</mapper>
